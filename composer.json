{
    "name": "czim/laravel-filter",
    "description": "Filter for Laravel Eloquent queries, with support for modular filter building",
    "keywords": [
        "laravel",
        "filter",
        "filters",
        "eloquent",
        "database"
    ],
    "homepage": "https://github.com/czim/laravel-filter",
    "license": "MIT",
    "authors": [
        {
            "name": "Coen Zimmerman",
            "email": "coen.zimmerman@endeavour.nl",
            "homepage": "https://endeavour.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^8.1",
        "illuminate/support": "^10 || ^11 || ^12",
        "illuminate/database": "^10 || ^11 || ^12",
        "myclabs/php-enum": "^1.8"
    },
    "require-dev": {
        "phpunit/phpunit" : "^10.0",
        "orchestra/testbench": "^8.0",
        "nunomaduro/larastan": "^2.2",
        "phpstan/phpstan-mockery": "^1.1",
        "phpstan/phpstan-phpunit": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Czim\\Filter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Czim\\Filter\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
